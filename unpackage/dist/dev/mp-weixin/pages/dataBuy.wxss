page {
  background-color: #f0f2f6;
}
.dataBuy {
  width: 750rpx;
}
.dataAdder {
  width: 690rpx;
  padding: 30rpx 30rpx 0;
  background-color: #FFFFFF;
}
.dataAdder .dataUser {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-bottom: 16rpx;
}
.dataAdder .dataUser text {
  display: block;
  font-size: 26rpx;
  color: #343434;
}
.dataAdder .dataUser .tel {
  margin: 0 16rpx;
}
.dataAdder .dataUser .default {
  background-color: #fe7c1e;
  color: #ffffff;
  font-size: 20rpx;
  padding: 1rpx 10rpx;
  border-radius: 4rpx;
}
.dataAdder .adders {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.dataAdder .adders text {
  font-size: 26rpx;
  color: #343434;
  display: block;
  width: 630rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.dataAdder .adders .icondingwei {
  font-size: 36rpx;
  color: #343434;
  margin-right: 8rpx;
}
.shop {
  width: 690rpx;
  padding: 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-align-content: center;
          align-content: center;
  background-color: #FFFFFF;
}
.shop image {
  width: 152rpx;
  height: 106rpx;
  border-radius: 8rpx;
  margin-right: 24rpx;
}
.shop .left {
  width: 505rpx;
}
.shop .left text {
  display: block;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.shop .left .title {
  color: #343434;
  font-size: 28rpx;
}
.shop .left .type {
  color: #999999;
  font-size: 24rpx;
  margin: 6rpx 0;
}
.shop .left .price {
  color: #ff1d00;
  font-size: 22rpx;
}
.item {
  width: 690rpx;
  padding: 20rpx 30rpx;
  color: #343434;
  font-size: 28rpx;
  font-weight: 600;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  background-color: #FFFFFF;
  margin-top: 20rpx;
}
.submint {
  width: 670rpx;
  height: 80rpx;
  border-radius: 40rpx;
  border: none;
  background-color: #fe9359 !important;
  line-height: 80rpx;
  color: #ffffff !important;
  font-size: 28rpx;
  margin-top: 100rpx;
}
checkbox .wx-checkbox-input {
  border-radius: 50% !important;
  color: #ffffff !important;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  color: #fff;
  background: #fe7c1e;
}
.wx-checkbox-input.wx-checkbox-input-checked {
  border: none !important;
}
.tip {
  color: #999;
  font-size: 24rpx;
  text-align: center;
  display: block;
  margin: 20rpx auto;
}

