<view class="dataDetail"><swiper class="indexBanner" indicator-dots="{{true}}" autoplay="{{false}}" interval="{{3000}}" duration="{{1000}}"><block wx:for="{{dataInfo.banner}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><swiper-item class="swiper-item"><image src="{{imgUrl+item}}" mode></image></swiper-item></block></block></swiper><view class="dataInfo"><text class="title">{{dataInfo.title}}</text><text>{{dataInfo.introduce}}</text></view><view class="line"></view><view class="specs"><block wx:if="{{dataInfo.type!=undefined}}"><view class="specsList"><text>已选</text><view class="specsType"><block wx:if="{{dataInfo.type==1}}"><view>纸质版</view></block><block wx:if="{{dataInfo.type==2}}"><view>电子版</view></block></view></view></block><block wx:if="{{dataInfo.type!=2}}"><view class="specsList"><text>送至</text><view class="specsType"><block wx:if="{{addressList!=''}}"><text data-event-opts="{{[['tap',[['addAddress',[0]]]]]}}" bindtap="__e">{{(addressList.area||'')+(addressList.details||'')}}</text></block><block wx:else><text data-event-opts="{{[['tap',[['addAddress',[1]]]]]}}" bindtap="__e">请选择收货地址</text></block></view></view></block></view><view class="dataDate"><view class="head"><block wx:if="{{urlType=='datas'}}"><view data-event-opts="{{[['tap',[['navTap',[0]]]]]}}" class="{{[Inv==0?'cur':'']}}" bindtap="__e">资料详情</view></block><block wx:if="{{urlType=='books'}}"><view data-event-opts="{{[['tap',[['navTap',[0]]]]]}}" class="{{[Inv==0?'cur':'']}}" bindtap="__e">图书详情</view></block><view data-event-opts="{{[['tap',[['navTap',[1]]]]]}}" class="{{[Inv==1?'cur':'']}}" bindtap="__e">出版详情</view></view><block wx:if="{{Inv==0}}"><view class="dataList"><block wx:for="{{dataInfo.details}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><image mode="widthFix" src="{{imgUrl+item}}"></image></block></block></view></block><block wx:if="{{Inv==1}}"><view class="dataList"><image mode="widthFix" src="{{imgUrl+dataInfo.publish}}"></image></view></block></view><view data-event-opts="{{[['tap',[['goBuy',['$0'],['dataInfo.id']]]]]}}" class="bottom" bindtap="__e"><button class="buy" type>购买完整资料</button></view><view class="contact"><image src="../static/concat.png" mode></image><button type open-type="contact"></button></view></view>