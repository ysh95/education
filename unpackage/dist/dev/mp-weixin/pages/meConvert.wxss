.nav {
  width: 750rpx;
}
.head {
  width: 100%;
  height: 80rpx;
  background-color: #ffffff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border-bottom: 2rpx solid #DDDDDD;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  align-items: center;
  position: fixed;
  z-index: 10;
  top: 0;
  left: 0;
}
.navName {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.navName .iconfont {
  font-size: 18rpx;
  display: block;
  color: #bbb8bb;
  margin-left: 16rpx;
}
.inv-h-se > text,
.inv-h-se .iconfont {
  color: #fe7c1e !important;
}
.navName > text {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  font-size: 30rpx;
  color: #343434;
}
.navList {
  width: 750rpx;
  height: calc(100vh - 40px);
  position: absolute;
  left: 0;
  top: 80rpx;
  z-index: 9;
  background-color: rgba(0, 0, 0, 0.5);
}
.navList .itemBox {
  width: 690rpx;
  background-color: #ffffff;
  padding: 0 30rpx 20rpx;
}
.navList .itemBox text {
  display: block;
  color: #343434;
  font-size: 28rpx;
  padding: 20rpx 0 0 0;
}
.content {
  width: 690rpx;
  padding: 30rpx 30rpx 100rpx;
}
.content .item {
  width: 690rpx;
  height: 170rpx;
  margin-bottom: 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.content .item .right {
  width: 240rpx;
  height: 170rpx;
  background-color: #fecf59;
  border-radius: 20rpx 0 0 20rpx;
  /* 相对定位，方便让before和after伪元素绝对定位偏移 */
  position: relative;
  /* 把超出div的部分隐藏起来 */
  overflow: hidden;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-align-content: center;
          align-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.content .item .right .iconfont {
  color: #ffffff;
  font-size: 62rpx;
  margin-bottom: 10rpx;
}
.content .item .right text {
  color: #ffffff;
  font-size: 22rpx;
  text-align: center;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  display: block;
  width: 100%;
}
.content .item .left {
  width: 401rpx;
  background-color: #ffffff;
  padding: 0 24rpx;
  height: 170rpx;
  border-radius: 0 20rpx 20rpx 0;
}
.content .item .left .leftT {
  width: 100%;
  position: relative;
  padding: 10rpx 0 26rpx;
}
.content .item .left .leftT text {
  color: #343434;
  font-size: 30rpx;
  margin: 20rpx 0;
  display: block;
}
.content .item .left .leftB {
  height: 54rpx;
  border-top: 1rpx dashed #dddddd;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.content .item .left .leftB text {
  color: #999999;
  font-size: 22rpx;
}
.content .item .left .leftB .goUse {
  color: #fe9359;
}
.right:after {
  content: ' ';
  width: 0;
  height: 100%;
  /* 绝对定位进行偏移 */
  position: absolute;
  top: 14rpx;
  border-right: 14rpx dotted white;
  /* 偏移一个半径，让圆点的一半覆盖div */
  right: -7rpx;
}
.bottom {
  width: 690rpx;
  background-color: #ffffff;
  height: 60rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  padding: 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.bottom button {
  width: 300rpx;
  color: #fff;
  background-color: #ef9359;
  height: 68rpx;
  border-radius: 34rpx;
  font-size: 28rpx;
}
.bottom .more {
  background-color: #fff;
  color: #ef9359;
  border: 1rpx solid #ef9359;
}

